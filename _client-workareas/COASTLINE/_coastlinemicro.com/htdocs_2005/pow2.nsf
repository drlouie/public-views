		mylayer2.hide(); 
		searchLayer1.x == 50;
		searchLayer1.y == -135;
		searchLayer1.hide(); 
		searchLayer2.x == 295;
		searchLayer2.y == -135;
		searchLayer2.hide();
	}
}

//
