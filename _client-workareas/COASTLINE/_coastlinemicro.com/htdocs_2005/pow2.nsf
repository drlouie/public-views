// SEARCH BOX ONE - INVENTORY SEARCH
function toggleSearch1() {	
	if (is.ns5) { 
	searchLayer1.show(); 
	}
	if (searchLayer1.y <= -135) {
		toggleSearch1Down();
	}
	if (searchLayer1.y >= 0) {
