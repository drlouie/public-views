}

function toggleSearch2Down() {
	if (searchLayer2.y == -135) {		
		javascript:searchLayer2.slideTo(295,0,5,10)
	}
}

function toggleSearch2Up() {
	if (searchLayer2.y == 0) {
		javascript:searchLayer2.slideTo(295,-135,5,10)
	}
}
