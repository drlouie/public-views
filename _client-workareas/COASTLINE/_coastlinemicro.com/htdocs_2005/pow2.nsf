boxieoff = new Image();
boxieoff.src = "sharktank/pow/parts/boxie_off.gif";

function checkPosition() {	
	if (is.ns5) { 
	mylayer.show(); 
	mylayer2.show(); 
	}
	if (mylayer.x <= -215) {
		moveIn();
	}
	if (mylayer.x >= 0) {
		moveOut();
	}
}

function checkPositionUno() {	
	if (is.ns5) { 
		document.images['powbar'].src = "images/spacer.gif"; 
	}
	if (mylayer.x <= -215) {
		moveIn();
	}
	if (mylayer.x >= 0) {
		moveOut();
	}
}

function moveIn() {
	if (is.ns5) { document.images['powdrop'].src = "sharktank/pow/null.gif"; }
	else if (is.ns) { document.hiderBlock.document.images['powdrop'].src = "sharktank/pow/null.gif"; }
	else { document.images['powdrop'].src = "sharktank/pow/null.gif"; }
	javascript:mylayer2.slideTo(0,300,5,10,'moveIn2();checkImage()')
}
function moveIn2() {
	if (mylayer.x == -215 && mylayer2.x == 0) {		
	javascript:mylayer.slideTo(0,300,5,10,'setTimeout(\'killHider()\',1000);checkImage()')
	}
}

function moveOut() {
	javascript:mylayer.slideTo(-215,300,5,10,'moveOut2();checkImage()')
}
function moveOut2() {
	if (mylayer2.x == 0 && mylayer.x == -215) {
	javascript:mylayer2.slideTo(-215,300,5,10,'checkImage()')
	}
}
function killHider() {
	if (is.ns5) { document.images['powdrop'].src = "sharktank/pow/offed.gif"; }
	else if (is.ns) { }
	else { document.images['powdrop'].src = "sharktank/pow/offed.gif"; }
	moveOut2Only();
	checkImage();
}
function moveOut2Only() {
	javascript:mylayer2.slideTo(-215,300,5,10)
}

function showObject(obj) {
	if (ns) obj.visibility = "show"
	else if (ie) obj.visibility = "visible"
}

function hideObject(obj) {
	if (ns) obj.visibility = "hide"
	else if (ie) obj.visibility = "hidden"
}

function checkImage() {
	if (mylayer.x == 0) {
	document.images['powbar'].src = hideoff.src
	}
	else if (mylayer.x == -215) {
	document.images['powbar'].src = showoff.src
	}
}

function mouseOvered() {
	if (mylayer.x == 0) {
	document.images['powbar'].src = hideov.src
	}
	else if (mylayer.x == -215) {
	document.images['powbar'].src = showov.src
	}
}

function mouseOffed() {
	if (mylayer.x == 0) {
	document.images['powbar'].src = hideoff.src
	}
	else if (mylayer.x == -215) {
	document.images['powbar'].src = showoff.src
	}
}

function boxieOff(which) {
	if (is.ns5) { document.images[which].src = boxieoff.src; }
	else if (is.ns) { }
	else { document.images[which].src = boxieoff.src; }
}

function boxieOv(which) {
	if (is.ns5) { document.images[which].src = boxieov.src; }
	else if (is.ns) { }
	else { document.images[which].src = boxieov.src; }
}
